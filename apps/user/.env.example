# 数据库配置
DB_HOST=localhost
DB_PORT=5432
DB_USER=postgres
DB_PASSWORD=postgres
DB_NAME=xcoding_user
DB_SSLMODE=disable

# gRPC配置
GRPC_ADDRESS=0.0.0.0
GRPC_PORT=50051

# HTTP配置
HTTP_ADDRESS=0.0.0.0
HTTP_PORT=10051

# 日志配置
LOG_LEVEL=info
LOG_FORMAT=json

# JWT配置
JWT_SECRET=your-secret-key

# 监控配置
METRICS_ENABLED=true
METRICS_PATH=/metrics

# 健康检查配置
HEALTH_CHECK_ENABLED=true
HEALTH_CHECK_PATH=/health